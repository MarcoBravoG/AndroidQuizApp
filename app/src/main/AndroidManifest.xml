<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pars.androidquizapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".HomeActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CategoryActivity" />
        <activity android:name=".category.view.AddCategoryActivity" />
        <!--<activity android:name=".CategoriesActivity" />-->
        <activity android:name=".question.view.AddQuestionActivity" />
        <!--<activity android:name=".QuestionsActivity" />-->
        <activity android:name=".MainGameActivity" />
        <activity android:name=".QuizResultActivity" />

    </application>

</manifest>